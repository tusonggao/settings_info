######################## alias by tusonggao BEGIN #################################
alias untar='tar -zxvf '                  # 解压命令
alias wget='wget -c '                     # 下载，支持断点续传
alias ping='ping -c 5'                    # 限制ping操作值进行6次
alias sha='shasum -a 256 '                # 检查某个文件的md5值
alias www='python3 -m http.server 6789'   # 临时共享某个文件夹
alias ipe='curl ipinfo.io/ip'             # 获得公网ip
alias c='clear'                           # 清屏命令
######################## alias by tusonggao END   ##################################


